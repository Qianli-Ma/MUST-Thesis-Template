\RequirePackage{eso-pic}
\newcommand\addBackgroundPic[1]{%
    \AddToShipoutPicture{
        \put(0,0){%
            \parbox[b][\paperheight]{\paperwidth}{%
                \vfill
                \centering
                \includegraphics[width=9.9cm,keepaspectratio]{#1}%
                \vfill}
        }
    }
}


